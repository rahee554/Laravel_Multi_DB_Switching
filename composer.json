{
    "name": "artflow-studio/laravel-dynamic-db",
    "type": "library",
    "description": "A Laravel package for dynamic Database Switching.",
    "keywords": ["laravel", "Database", "Multi Tenant"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "ArtflowStudio\\LaravelDynamicDb\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },    
    "extra": {
        "laravel": {
            "providers": [
                "ArtflowStudio\\LaravelDynamicDb\\DatabaseServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
